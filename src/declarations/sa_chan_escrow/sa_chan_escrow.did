type Time = int;
type SubaccountBlob = blob;
type Result_1 = 
 variant {
   err: text;
   ok;
 };
type Result = 
 variant {
   err: text;
   ok: AccountIdText;
 };
type ProjectState = 
 variant {
   approved: vec principal;
   closed;
   live;
   noproject;
 };
type ProjectId__1 = nat;
type ProjectId = nat;
type EscrowCanister = 
 service {
   addConfirmedAccountsToConfirmedAccountsArray: () -> ();
   cancelTransfer: (AccountIdText) -> ();
   confirmTransfer: (AccountIdText) -> (Result_1);
   getAccountsInfo: () -> (text) query;
   getConfirmedAccountsArray: () -> (vec AccountIdAndTime) query;
   getDisbursements: () -> (text) query;
   getLogs: () -> (text) query;
   getMetadata: () ->
    (record {
       projectId: ProjectId__1;
       recipient: principal;
     }) query;
   getNewAccountId: (principal) -> (Result);
   getProjectState: () -> (ProjectState) query;
   getSubaccountsInfo: () ->
    (record {
       toDrain: record {
                  arr: vec APS;
                  count: nat;
                  index: nat;
                };
       toRefund: record {
                   arr: vec APS;
                   count: nat;
                   index: nat;
                 };
     }) query;
   releaseFunds: () -> ();
   returnFunds: () -> ();
   updateProjectState: () -> (ProjectState);
 };
type AccountIdText = text;
type AccountIdAndTime = 
 record {
   accountId: AccountIdText;
   time: Time;
 };
type APS = 
 record {
   AccountIdText;
   principal;
   SubaccountBlob;
 };
service : (ProjectId, principal) -> EscrowCanister
